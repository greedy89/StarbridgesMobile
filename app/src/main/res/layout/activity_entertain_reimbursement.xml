<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.EntertainReimbursementActivity">
<ListView
    android:id="@+id/lvEntertainReimburse"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="5dp"
    android:paddingTop="5dp"
    android:paddingRight="5dp"></ListView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fabEntertainReimburse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="15dp"
        android:clickable="true"
        android:focusable="true"
        app:srcCompat="@drawable/ic_menu_add"
        app:backgroundTint="@color/wallet_holo_blue_light"
        app:fabSize="normal"  />
</RelativeLayout>